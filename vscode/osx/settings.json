// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "'Fira Code', FiraFlott, 'Operator Mono', 'OperatorMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 800,
  "editor.useTabStops": false,
  "editor.renderWhitespace": "all",
  "editor.lineHeight": 22,
  "editor.tabCompletion": "on",
  "editor.fontLigatures": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.autoIndent": "none",
  "editor.fontWeight": "500",
  "editor.quickSuggestionsDelay": 5,
  "editor.formatOnPaste": false,
  "editor.insertSpaces": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.rulers": [100],
  "editor.rename.enablePreview": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.defaultLocation": "right",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.autoSave": "off",
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.vue": "vue",
    "*.gs": "javascript",
    ".stylelintrc": "json",
    "*.mdx": "mdx"
  },
  "files.exclude": {
    ".git": true,
    "**/.sass-cache": true,
    "**/.tmp": true,
    "**/*.gz": true,
    "**/*.nyc_output": true,
    "**/*.tar": true,
    "**/*.zip": true,
    "**/node_modules": true,
    "api/**/*": true,
    "node_modules/**/*": true
  },
  "html.format.wrapLineLength": 100,
  "html.format.endWithNewline": true,
  "extensions.autoUpdate": true,
  "telemetry.enableTelemetry": false,
  "html.format.indentHandlebars": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.preferences": {
    "filter.commentAfter": "\n<!-- /[#ID][.CLASS] -->"
  },
  "emmet.includeLanguages": {
    "blade": "html",
    "html": "html",
    "erb": "html",
    "edge": "html",
    "ts": "javascript",
    "javascript": "javascriptreact",
    "vue": "html",
    "php": "html",
    "gs": "javascript",
    "vue-html": "html",
    "ejs": "html"
  },
  "emmet.syntaxProfiles": {
    "javascript": "javascriptreact",
    "sass": "css",
    "gs": "javascript",
    "vue-html": "html",
    "vue": "html",
    "php": "html",
    "erb": "html"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "git.showCommitInput": true,
  "trailing-spaces.trimOnSave": true,
  "material-icon-theme.showUpdateMessage": false,
  "typescript.check.npmIsInstalled": false,
  "extensions.ignoreRecommendations": true,
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "eslint.migration.2_x": "off",
  "eslint.validate": ["javascript", "ts", "typescript", "javascriptreact", "vue"],
  "eslint.options": {
    "extensions": [".html", ".js", ".vue", ".jsx"]
  },
  "vetur.format.defaultFormatter.js": "vscode-typescript",
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "eslint.format.enable": true,
  "vetur.validation.script": false,
  "vetur.validation.template": false,
  "scss.validate": false,
  "css.validate": false,
  "less.validate": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.completeJSDocs": true,
  "javascript.format.enable": false,
  "javascript.autoClosingTags": true,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "javascript.referencesCodeLens.enabled": true,
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "npm.packageManager": "yarn",
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "prettier.printWidth": 100,
  "prettier.ignorePath": ".gitignore,.prettierignore",
  "material-icon-theme.activeIconPack": "vue_vuex",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": true,
  "[php]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[vue]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "javascript.format.semicolons": "insert",
  "search.actionsPosition": "right",
  "search.collapseResults": "auto",
  "search.quickOpen.includeHistory": false,
  "search.smartCase": true,
  "search.showLineNumbers": true,
  "search.exclude": {
    "**/build": true,
    "**/__*": true,
    ".temp": true,
    "temp": true,
    ".tmp": true,
    "tmp": true,
    "**/*.min.*": true,
    "**/bower": true,
    "**/.tmp": true,
    "**/_bower": true,
    "**/_site": true,
    "**/dev": true,
    "**/local": true,
    "**/prod": true,
    "**/.nuxt": true,
    "**/dist": true,
    "**/wp-admin": true,
    "**/wp-includes": true,
    "**/node_modules": true,
    "**/.sass-cache": true
  },
  "window.zoomLevel": 1,
  "window.titleSeparator": "  ðŸ’»  ",
  "liveServer.settings.donotShowInfoMsg": true,
  "rest-client.defaultHeaders": {
    "User-Agent": "vscode-restclient",
    "content-type": "application/json"
  },
  "editor.fontSize": 11,
  "todo-tree.tree.showScanModeButton": false,
  "vscodeGoogleTranslate.preferredLanguage": "Portuguese",
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false
}
