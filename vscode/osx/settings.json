// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "Fira Code, FiraFlott, 'Operator Mono', 'OperatorMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 800,
  "editor.useTabStops": false,
  "editor.fontSize": 12,
  "editor.lineHeight": 24,
  "editor.semanticHighlighting.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.tabCompletion": "on",
  "editor.fontLigatures": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.autoIndent": "none",
  "editor.fontWeight": "500",
  "editor.quickSuggestionsDelay": 5,
  "editor.formatOnPaste": false,
  "editor.insertSpaces": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.rulers": [80, 120],
  "editor.rename.enablePreview": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.colorTheme": "New Moon",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.defaultLocation": "right",
  "workbench.sideBar.location": "left",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.autoSave": "off",
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.vue": "vue",
    "*.gs": "javascript",
    ".stylelintrc": "json",
    "*.mdx": "mdx",
    "*.vue.hbs": "vue",
    "*.js.hbs": "javascript",
    "*.ts.hbs": "typescript",
    "*.tsx.hbs": "typescriptreact",
    "*.jsx.hbs": "javascriptreact",
    "*.css.hbs": "css",
    "*.scss.hbs": "scss",
    "*.json.hbs": "json",
    "*.md.hbs": "markdown",
    ".env.template": "dotenv",
    ".env.sample": "dotenv",
    ".sequelizerc": "javascript",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact"
  },
  "files.exclude": {
    ".git": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/.sass-cache": true,
    "**/.tmp": true,
    "**/*.gz": true,
    "**/*.nyc_output": true,
    "**/*.tar": true,
    "**/*.zip": true,
    "**/node_modules": true,
    "node_modules/**/*": true
  },
  "html.format.wrapLineLength": 100,
  "html.format.endWithNewline": true,
  "extensions.autoUpdate": true,
  "telemetry.enableTelemetry": false,
  "html.format.indentHandlebars": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.preferences": {
    "filter.commentAfter": "\n<!-- /[#ID][.CLASS] -->"
  },
  "emmet.includeLanguages": {
    "blade": "html",
    "html": "html",
    "erb": "html",
    "edge": "html",
    "ts": "javascript",
    "javascript": "javascriptreact",
    "vue": "html",
    "php": "html",
    "gs": "javascript",
    "vue-html": "html",
    "ejs": "html"
  },
  "emmet.syntaxProfiles": {
    "javascript": "javascriptreact",
    "sass": "css",
    "gs": "javascript",
    "vue-html": "html",
    "vue": "html",
    "php": "html",
    "erb": "html"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.ignoreMissingGitWarning": true,
  "git.showCommitInput": true,
  "trailing-spaces.trimOnSave": true,
  "extensions.ignoreRecommendations": true,
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "eslint.migration.2_x": "off",
  "eslint.validate": ["javascript", "ts", "typescript", "javascriptreact", "vue"],
  "eslint.options": {
    "extensions": [".html", ".js", ".vue", ".jsx"]
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "eslint.format.enable": true,
  "vetur.validation.script": false,
  "vetur.validation.template": false,
  "scss.validate": false,
  "css.validate": false,
  "less.validate": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.completeJSDocs": true,
  "javascript.format.enable": false,
  "javascript.autoClosingTags": true,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "javascript.referencesCodeLens.enabled": true,
  "npm.packageManager": "yarn",
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.validate.enable": false,
  "typescript.tsserver.log": "off",
  "typescript.validate.enable": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.check.npmIsInstalled": false,
  "prettier.printWidth": 100,
  "prettier.enableDebugLogs": true,
  "prettier.singleQuote": true,
  "prettier.ignorePath": ".gitignore,.prettierignore",
  "material-icon-theme.showUpdateMessage": false,
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "scripts",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes"
  },
  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "tsconfig.json": "tune",
    "*.proto": "3d",
    "*.webpack.js": "webpack"
  },
  "explorer.confirmDelete": true,
  "[php]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.quickSuggestions": true,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "javascript.format.semicolons": "insert",
  "search.actionsPosition": "right",
  "search.collapseResults": "auto",
  "search.quickOpen.includeHistory": false,
  "search.smartCase": true,
  "search.showLineNumbers": true,
  "search.exclude": {
    "**/out": true,
    "**/build": true,
    "**/dist": true,
    "**/__*": true,
    "**/*.lock": true,
    ".temp": true,
    "temp": true,
    ".tmp": true,
    "tmp": true,
    "**/*.min.*": true,
    "**/bower": true,
    "**/.tmp": true,
    "**/_bower": true,
    "**/_site": true,
    "**/dev": true,
    "**/local": true,
    "**/prod": true,
    "**/.nuxt": true,
    "**/.next": true,
    "**/wp-admin": true,
    "**/wp-includes": true,
    "**/node_modules": true,
    "**/.sass-cache": true
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "rest-client.defaultHeaders": {
    "User-Agent": "vscode-restclient",
    "content-type": "application/json"
  },
  "todo-tree.tree.showScanModeButton": false,
  "vscodeGoogleTranslate.preferredLanguage": "Portuguese",
  "files.insertFinalNewline": true,
  "workbench.editor.decorations.badges": true,
  "gist.defaultPrivate": true,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.productIconTheme": "fluent-icons",
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "[ ]", "[x]"],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "editor.largeFileOptimizations": false,
  "terminal.integrated.fontSize": 12,
  "terminal.external.osxExec": "Hyper.app",
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.automationShell.osx": "/bin/zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.env.osx": {
    "NODE_ENV": "development"
  },
  "terminal.integrated.tabs.enabled": true,
  "cssrem.addMark": true,
  "cssrem.fixedDigits": 5,
  "vsicons.dontShowNewVersionMessage": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tabnine.experimentalAutoImports": true,
  "editor.suggestSelection": "first",
  "splitHTMLAttributes.closingBracketOnNewLine": true,
  "window.title": "${activeEditorShort} ${separator} ${rootName}",
  "window.zoomLevel": 1
}